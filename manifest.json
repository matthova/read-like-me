{
    "name": "Read Like Me",
    "version": "1.0",
    "description": "Share your alphabet of grapheme synesthesia with this chrome extension.",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["style.css"],
        "js": ["content.js", "jquery.min.js", "button.js"]
    }],
    "browser_action": {
        "scripts": ["popup.js"],
        "default_title": "Read Like Me",
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab"
    ],
    "manifest_version": 2
}
